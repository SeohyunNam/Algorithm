        {
            "name": "(gdb) 연결",
            "type": "cppdbg",
            "request": "attach",
            "program": "프로그램 이름 입력(예: ${workspaceFolder}/a.exe)",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "miDebuggerPath": "/path/to/gdb",
            "setupCommands": [
                {
                    "description": "gdb에 자동 서식 지정 사용",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },      
        {
            "type": "java",
            "name": "Launch App",
